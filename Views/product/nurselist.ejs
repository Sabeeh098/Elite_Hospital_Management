<div class="content">
  <div class="page-header">
    <div class="page-title">
      <h4>Nurses</h4>
      <h6>Manage your Nurses</h6>
    </div>
  </div>

  <!-- /product list -->
  <div class="card">
    <div class="card-body">
      <div class="table-responsive">
        <table class="table datanew">
          <thead>
            <tr>
              <th>S. No</th>
              <th>Patient Name</th>
              <th>Email</th>
              <th>Address</th>
              <th>Gender</th>
              <th>Qualification</th>
            </tr>
          </thead>
          <tbody>
            <% nurses.forEach((nurse, index) => {  %>
            <tr>
              <td><%= index + 1 %></td>
              <td><%= nurse.nurseName %></td>
              <td><%= nurse.email %></td>
              <td><%= nurse.address %></td>
              <td><%= nurse.gender%></td>
              <td><%= nurse.qualification%></td>
            </tr>
            <% }); %> 
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
